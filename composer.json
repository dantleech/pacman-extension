{
    "name": "phpactor/pacman-extension",
    "description": "Manage your project packages",
    "license": "MIT",
    "type": "phpactor-extension",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.2",
        "phpactor/console-extension": "dev-master",
        "phpactor/composer-autoloader-extension": "dev-master",
        "composer/composer": "^1.7",
        "phpactor/source-code-filesystem-extension": "dev-master",
        "phpactor/worse-reflection-extension": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Pacman\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Pacman\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Pacman\\Package Manager ExtensionExtension"
    }
}
